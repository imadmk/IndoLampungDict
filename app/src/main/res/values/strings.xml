<resources>

    <!-- The name of the application. -->
    <string name="app_name">Lampung - Indonesia Dict</string>

    <!-- The label for use as a searchable item -->
    <string name="search_label">Pencarian</string>

    <!-- The hint text that appears in the search box. -->
    <string name="search_hint">Cari terjemahan</string>

    <!-- The menu entry that invokes search. -->
    <string name="menu_search">Pencarian</string>

    <!-- The description that will show up in the search settings for this source.  -->
    <string name="settings_description">Arti Kata</string>

    <!-- General instructions in the main activity. -->
    <string name="search_instructions">Gunakan Search Box pada Action Bar untuk memasukkan kata</string>

    <!-- Shown above search results when we receive a search request. -->
    <plurals name="search_results">
      <item quantity="one">%1$d kata ditemukan untuk \"%2$s\": </item>
      <item quantity="other">%1$d kata ditemukan untuk \"%2$s\": </item>
    </plurals>

    <!-- Search failure message. -->
    <string name="no_results">Tidak ditemukan terjemahan untuk \"%s\"</string>
    <string name="translation_word">Arti Kata</string>
    <string name="trans_dialek_o">Bahasa Lampung Dialek O :</string>
    <string name="trans_dialek_a">Bahasa Lampung Dialek A :</string>
    <string name="word_list">Daftar kata</string>

</resources>
